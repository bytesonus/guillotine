{
	"version": "1.0.0",
	"configs": [
		{
			"env": {
				"target_family": "windows",
				"target_os": "windows",
				"target_arch": "x86_64",
				"target_endian": "little"
			},
			"config": {
				"gotham": {
					"path": "./service/gotham",
					"connection_type": "inet_socket",
					"port": 2203,
					"bind-addr": "127.0.0.1"
				},
				"modules": "./modules"
			}
		},
		{
			"env": {
				"target_family": "unix",
				"target_os": "linux",
				"target_arch": "x86_64",
				"target_endian": "little"
			},
			"config": {
				"gotham": {
					"path": "./service/gotham",
					"connection_type": "unix_socket",
					"socket_path": "./gotham.sock"
				},
				"modules": "./modules"
			}
		}
	]
}