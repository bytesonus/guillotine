{
	"version": 1,
	"configs": [
		{
			"target": {
				"family": "windows",
				"os": "windows",
				"arch": "x86_64",
				"endian": "little"
			},
			"config": {
				"name": "runner1",
				"logs": "./logs",
				"host": {
					"path": "./service/juno",
					"connection_type": "inet_socket",
					"port": 2203,
					"bind_addr": "127.0.0.1"
				}
			}
		},
		{
			"target": {
				"family": "unix",
				"os": "linux",
				"arch": "x86_64",
				"endian": "little"
			},
			"config": {
				"name": "runner1",
				"logs": "./logs",
				"host": {
					"path": "./service/juno",
					"connection_type": "unix_socket",
					"socket_path": "./juno.sock"
				},
				"modules": {
					"directory": "./modules"
				}
			}
		},
		{
			"config": {
				"name": "runner2",
				"logs": "./logs",
				"node": {
					"connection_type": "inet_socket",
					"port": 2203,
					"ip": "10.0.0.1"
				},
				"modules": {
					"directory": "./modules"
				}
			}
		}
	]
}